                                    </button>
                                    <button class="btn btn-navbar" type="button" data-widget="navbar-search">
                                        <i class="fas fa-times"></i>
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                </li>*@

                <!-- Messages Dropdown Menu -->
                <li class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#">
                        <i class="far fa-comments"></i>
                        <span class="badge badge-danger navbar-badge">@messages.Count</span>
                    </a>
                    @{
                        var cnt = 4; // will be shown only 3 fresh messages
                        if (ViewBag.IsShownAllMsg == true)
                        {
                            cnt = messages.Count + 1;
                        }
                    }
                    <div class="dropdown-menu dropdown-menu-lg dropdown-menu-right">
                        @foreach (var msg in currentMessages)
                        {
                            --cnt;
                            if (cnt == 0)
                            {
                                break;
                            }
                            var sender = users.FirstOrDefault(u => u.UserId == msg.SenderId);
                            <a href="/Admin/_829528a_441d_484m_862i_22475963ffdn?isPartView=true" class="dropdown-item">
                                <!-- Message Start -->
                                <div class="media" style="width:300px">
                                    <img src="~/Content/AdminPanel/dist/img/user1-128x128.jpg" alt="User Avatar" class="img-size-50 mr-3 img-circle">
                                    <div class="media-body">
                                        <h3 class="dropdown-item-title">
                                            @if (sender.Role == "Guest")
                                            {
                                                <span>@sender.FirstName</span>
                                            }
                                            else
                                            {
                                                <span>@sender.FirstName&nbsp;@sender.LastName</span>
                                            }

                                            @if (!msg.IsReviwed) // if message not reviewed
                                            {
                                                <span class=" text-sm text-warning"><i class="fas fa-star"></i></span>
                                            }
                                            else
                                            {
                                                <span class="text-sm text-danger"><i class="fas fa-star"></i></span>
                                            }
                                        </h3>
                                        <p class="text-sm">@msg.Title...</p>
                                        @{
                                            TimeSpan diff = DateTime.Now - msg.DateMessage;
                                            var leftHours = (int)diff.TotalHours;
                                        }
                                        <p class="text-sm text-muted">
                                            <i class="far fa-clock mr-1">
                                            </i>@leftHours Hours Ago
                                        </p>
                                    </div>
                                </div>
                                <!-- Message End -->
                            </a>
                            <div class="dropdown-divider"></div>
                        }

                        @if (ViewBag.IsShownAllMsg == false)
